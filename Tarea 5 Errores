int main()
{
  float cal1 = 0, cal2 = 0, cal3 = 0, cal4 = 0, cal5 = 0, Prom = 0;
  string N;

  cout << "Escribe las 5 calificaciones del alumno:""\n";
  cout << "Calificacion 1:""\n";
    cin >> cal1;
    cout << "Calificacion 2:""\n";
    cin >> cal2;
