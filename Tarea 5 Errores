    cin >> cal3;
    cout << "Calificacion 4:""\n";
    cin >> cal4;
    cout << "Calificacion 5:""\n";
    cin >> cal5;
 
    Prom = (cal1 + cal2 + cal3 + cal4 + cal5) / 5;


    cout << "El promedio final del alumno es" << Prom;
